<div class="middle-header-content">
  <div class="container">
    <div class="middle-header-wrapper">
      <div class="middle-header-left-wrapper">
        {% liquid 
        if enable_currency 
          render 'currency'
        endif
        if enable_language
          render 'languages'
        endif 
        %}
        {%- if section.settings.enable_socials -%}
          <div class="header-social">
            <ul>
              {%- if settings.social_facebook_link != blank -%}
                <li class="list-social__item">
                  <a target="_blank" href="{{ settings.social_facebook_link }}" class="no-effect">
                    <i class="icon-fb"></i>
                    <span class="visually-hidden">{{ 'general.social.links.facebook' | t }}</span>
                  </a>
                </li>
              {%- endif -%}
              {%- if settings.social_twitter_link != blank -%}
                <li class="list-social__item">
                  <a target="_blank" href="{{ settings.social_twitter_link }}" class="no-effect">
                    <i class="icon-tw"></i>
                    <span class="visually-hidden">{{ 'general.social.links.twitter' | t }}</span>
                  </a>
                </li>
              {%- endif -%}
              {%- if settings.social_pinterest_link != blank -%}
                <li class="list-social__item">
                  <a target="_blank" href="{{ settings.social_pinterest_link }}" class="no-effect">
                    <i class="icon-p"></i>
                    <span class="visually-hidden">{{ 'general.social.links.pinterest' | t }}</span>
                  </a>
                </li>
              {%- endif -%}
              {%- if settings.social_instagram_link != blank -%}
                <li class="list-social__item">
                  <a target="_blank" href="{{ settings.social_instagram_link }}" class="no-effect">
                    {%- render 'icon-instagram' -%}
                    <span class="visually-hidden">{{ 'general.social.links.instagram' | t }}</span>
                  </a>
                </li>
              {%- endif -%}
              {%- if settings.social_tiktok_link != blank -%}
                <li class="list-social__item">
                  <a target="_blank" href="{{ settings.social_tiktok_link }}" class="no-effect">
                    {%- render 'icon-tiktok' -%}
                    <span class="visually-hidden">{{ 'general.social.links.tiktok' | t }}</span>
                  </a>
                </li>
              {%- endif -%}
              {%- if settings.social_tumblr_link != blank -%}
                <li class="list-social__item">
                  <a target="_blank" href="{{ settings.social_tumblr_link }}" class="no-effect">
                    {%- render 'icon-tumblr' -%}
                    <span class="visually-hidden">{{ 'general.social.links.tumblr' | t }}</span>
                  </a>
                </li>
              {%- endif -%}
              {%- if settings.social_snapchat_link != blank -%}
                <li class="list-social__item">
                  <a target="_blank" href="{{ settings.social_snapchat_link }}" class="no-effect">
                    {%- render 'icon-snapchat' -%}
                    <span class="visually-hidden">{{ 'general.social.links.snapchat' | t }}</span>
                  </a>
                </li>
              {%- endif -%}
              {%- if settings.social_youtube_link != blank -%}
                <li class="list-social__item">
                  <a target="_blank" href="{{ settings.social_youtube_link }}" class="no-effect">
                    {%- render 'icon-youtube' -%}
                    <span class="visually-hidden">{{ 'general.social.links.youtube' | t }}</span>
                  </a>
                </li>
              {%- endif -%}
              {%- if settings.social_vimeo_link != blank -%}
                <li class="list-social__item">
                  <a target="_blank" href="{{ settings.social_vimeo_link }}" class="no-effect">
                    {%- render 'icon-vimeo' -%}
                    <span class="visually-hidden">{{ 'general.social.links.vimeo' | t }}</span>
                  </a>
                </li>
              {%- endif -%}
            </ul>
          </div>
        {% endif %}
      </div>
      {% render 'header-logo' %}
      {% render 'header-search' %}
      <div class="middle-header-right-wrapper">
        <div class="child-content header__icons">
          {% render 'header-search' %}
          {%- if shop.customer_accounts_enabled -%}
            <a href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}" class="no-effect header__icon header__icon--account link focus-inset" title="{{ 'customer.account.title' | t }}">
              <span class="icon-user"></span>
              <span class="visually-hidden">
              {%- liquid
                if customer
                  echo 'customer.account_fallback' | t
                else
                  echo 'customer.log_in' | t
                endif
              -%}
            </span>
            </a>
          {%- endif -%}
          <div class="header-cart">
            {% if template != 'cart' and settings.cart_drawer_enable %}
              <modal-opener data-modal="#Slideout-cart">
                <a href="{{ routes.cart_url }}" class="no-effect action-cart header__icon icon-cart header__icon--cart" id="cart-icon-bubble" aria-label="{{ 'templates.cart.cart' | t }}" title="{{ 'templates.cart.cart' | t }}">
                  <span class="visually-hidden">{{ 'templates.cart.cart' | t }}</span>
                  {% if settings.cart_dialog_enable %}
                    <div class="cart-count-bubble">
                      <span class="counter-number" aria-hidden="true">{% if cart.item_count > 99 %}99+{% else %}{{ cart.item_count }}{% endif %}</span>
                      <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                    </div>
                  {% endif %}
                </a>
              </modal-opener>
              <cart-drawer class="minicart-content cart-slideout" id="Slideout-cart">
                <div class="cart-slideout-inner">
                  <div class="header-cart-title">
                    <div class="h3 sub-text">Cart</div>
                    <button class="btn-minicart-close icon-cancel" id="ModalClose-Slideout-cart" aria-label="{{ 'sections.header.close_cart' | t }}"></button>
                  </div>
                  <div class="minicart-slideout-wrapper{% if settings.cart_note_enable %} have-note{% endif %}">
                    
                  </div>
                </div>
              </cart-drawer>
            {% else %}
              <a href="{{ routes.cart_url }}" class="no-effect action-cart header__icon icon-cart header__icon--cart" id="cart-icon-bubble" aria-label="{{ 'templates.cart.cart' | t }}" title="{{ 'templates.cart.cart' | t }}">
                <span class="visually-hidden">{{ 'templates.cart.cart' | t }}</span>
                {% if settings.cart_dialog_enable %}
                  <div class="cart-count-bubble">
                    <span class="counter-number" aria-hidden="true">{% if cart.item_count > 99 %}99+{% else %}{{ cart.item_count }}{% endif %}</span>
                    <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                  </div>
                {% endif %}
              </a>
            {% endif %}
          </div>
        </div>
        {% render 'slideout-menu' %}
      </div>
    </div>
  </div>
</div>