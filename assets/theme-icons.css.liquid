@font-face {
	font-family: 'cecile-icon';
	src : url("{{ 'cecile-icon.eot' | asset_url }}");
	src : url("{{ 'cecile-icon.eot' | asset_url }}#iefix") format('embedded-opentype'),
	url("{{ 'cecile-icon.ttf' | asset_url }}") format('truetype'),
	url("{{ 'cecile-icon.woff' | asset_url }}") format('woff'),
	url("{{ 'cecile-icon.svg' | asset_url }}#cecile-icon") format('svg');
	font-weight: normal;
	font-style: normal;
	font-display: block;
}

[class^="icon-"],
[class*=" icon-"] {
	/* use !important to prevent issues with browser extensions that change fonts */
	font-family: 'cecile-icon' !important;
	speak: never;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

