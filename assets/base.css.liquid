.tooltip{
  position: relative;
}
.tooltip:after,
.tooltip:before {
  content                 : "";
  position                : relative;
  -webkit-transform       : translate3d(0, 0, 0);
  -moz-transform          : translate3d(0, 0, 0);
  transform               : translate3d(0, 0, 0);
  pointer-events          : none;
  border-radius           : 4px;
  transition              : opacity 0.3s ease, visibility 0.3s ease, transform 0.3s cubic-bezier(0.71, 1.7, 0.77, 1.24);
  -o-transition: opacity 0.3s ease, visibility 0.3s ease, -webkit-transform 0.3s cubic-bezier(0.71, 1.7, 0.77, 1.24);
  -moz-transition: opacity 0.3s ease, visibility 0.3s ease, -webkit-transform 0.3s cubic-bezier(0.71, 1.7, 0.77, 1.24);
  -webkit-transition: opacity 0.3s ease, visibility 0.3s ease, -webkit-transform 0.3s cubic-bezier(0.71, 1.7, 0.77, 1.24);
  -ms-transition: opacity 0.3s ease, visibility 0.3s ease, -webkit-transform 0.3s cubic-bezier(0.71, 1.7, 0.77, 1.24);
  transition-delay        : 0s;
  -o-transition-delay: 0s;
  -moz-transition-delay: 0s;
  -webkit-transition-delay: 0s;
  -ms-transition-delay: 0s;
}

.tooltip:before {
  background  : 0 0;
  border      : 8px solid transparent;
  border-color: #595959 transparent transparent transparent;
  left: calc(50% + 2px);
  transform: translateX(-50%);
  top: calc(100% - 2px);
  position    : absolute;
}

.tooltip:after {
  content       : attr(title);
  background    : #595959;
  color         : #e8e8e8;
  padding       : 12px 19px;
  font-size     : 16px;
  white-space   : pre-wrap;
  text-align    : center;
  display       : block;
  min-width     : 116px;
  letter-spacing: 0em;
}
.style-2.tooltip:after , .style-1.tooltip:after {
  padding: 12px 16px;
}
.tooltip.style-1 {
  margin-right: 36px;
}

.tooltip.style-1:before {
  border-color: transparent transparent transparent #595959;
  right       : 0;
  left        : auto;
  top         : 50%;
  transform   : translate(13px, -50%);
}

.tooltip.style-2:before {
  border-color: transparent #595959 transparent transparent;
  left        : 0;
  top         : 50%;
  transform   : translate(-14px, -50%);
}

.tooltip.style-4:before {
  border-color: transparent transparent #595959 transparent;
  top         : -14px;
}

